font_family      Jetbrains mono nerd font
font_size        10
# the default is 12   
bold_font        auto
italic_font      auto
bold_italic_font auto
mouse_hide_wait  2.0
cursor_shape     block
# url_color        #0087bd
url_style        dotted
enable_audio_bell no

# Cursor Trail 
cursor_trail 1

# close the terminal without confirmation
confirm_os_window_close 0 
background_opacity 0.60

# BEGIN_KITTY_THEME
# Ros√© Pine
include current-theme.conf
# END_KITTY_THEME
